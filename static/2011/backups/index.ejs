<div id="ex3a" class="jqmDialog">
	<div class="jqmdTL"><div class="jqmdTR"><div class="jqmdTC jqDrag">キャラクター追加</div></div></div>
	<div class="jqmdBL"><div class="jqmdBR"><div class="jqmdBC">
	<div class="jqmdMSG">
		<h2>キャラクタ作成が出来ます</h2>
		<h5>ツイッターやフェイスブックのアカウントのフローフィル写真を使います。</h5>
        &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;<input type="radio" name="chk_info" value="1" onclick="chk_radio()"checked="checked">Twitter&nbsp;
		<input type="radio" name="chk_info" value="2" onclick="chk_radio()">Facebook&nbsp;<br />
        <script language="javascript">
		var flag1 = 0;
		var flag2 = 0;
		var pic_url;
		var more = 0;
		var socket = io.connect();

		function check_submit1() {
			if (document.myForm.name.value == "") {
				 alert('アカウントを入力して下さい');
				 document.myForm.name.focus();
				 return;
			}else {
				var input = document.getElementById("picture");
				pic_url = "http://img.tweetimag.es/i/"+ document.myForm.name.value +"_n";
				input.innerHTML = "&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;アカウント：<input type='text' value='" + document.myForm.name.value + "' disabled='disabled' size=15 maxlength=25><br />"+
				"&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;"+
				"<img src='" + pic_url + "' alt='" + document.myForm.name.value + "' onerror='error();' />";
				flag1 = 1;
				flag2 = 0;
				check_ok();
			}
		}

		function check_submit2() {
			if (document.myForm2.name2.value == "") {
				 alert('アカウントを入力して下さい');
				 document.myForm2.name2.focus();
				 return;
			}else {
				var input = document.getElementById("picture");
				pic_url = "https://graph.facebook.com/"+ document.myForm2.name2.value +"/picture";
				input.innerHTML = "&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;アカウント：<input type='text' value='" + document.myForm2.name2.value + "' disabled='disabled' size=15 maxlength=25><br />"+
				"&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;"+
				"<img src='" + pic_url + "' onerror='error();' />";
				flag1 = 0;
				flag2 = 1;
				check_ok();
			}
		}


		function check_submit3(){
			var select_sex;
			if(flag4==1){
				select_sex = "man.gif";
			}else if(flag5==1){
				select_sex = "girl.gif";
			}else if(flag6==1){
				select_sex = "sexless.gif";
			}
			if(more == 1){
				alert("２回目の追加はブラウザを更新してから出来ますが,\nキャラクタが多くなるとサーバーが重くなりますので\nしないで下さい・・・");
				document.myForm3.submitok.disabled = true;
			}else{
				socket.emit('account-add',{image:pic_url, sex:select_sex, name:document.myForm3.name3.value, name2:document.myForm3.name4.value});
				document.myForm3.submitok.disabled = true;
				alert("追加出来ました");
				more = 1;
			}
		}

		function error(){
			alert("正しく入力してください");
			flag1 = 0;
			flag2 = 0;
			check_ok();
		}

		function chk_radio(){
			var input = document.getElementById("picture");
			if(document.all.chk_info[0].checked == true){
				input.innerHTML = "<form name='myForm' method='post'>"+
				"&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;アカウント：<input type='text' name='name' size=15 maxlength=25 onKeyDown='javascript:if (event.keyCode == 13){ window.event.returnValue = false;}'>"+
				"<input type='button' value='確認' onclick='javascript:check_submit1();'> </form>";
				flag1=0;
				flag2=0;
				check_ok();
			}else if(document.all.chk_info[1].checked == true){
				input.innerHTML = "<form name='myForm2' method='post'>"+
				"&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;アカウント：<input type='text' name='name2' size=15 maxlength=25 onKeyDown='javascript:if (event.keyCode == 13){ window.event.returnValue = false;}'>"+
				"<input type='button' value='確認' onclick='javascript:check_submit2();'> </form>";
				flag1=0;
				flag2=0;
				check_ok();
			}
		}
        </script>

        <div id="picture">
			<form name='myForm' method='post'>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;アカウント：<input type='text' name='name' size=15 maxlength=25 onKeyDown='javascript:if (event.keyCode == 13){ window.event.returnValue = false;}'>
			<input type='button' value='確認' onclick='javascript:check_submit1();'> </form>
		</div>
        <br />
        &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;<input type="radio" name="chk_info2" value="0" onclick="chk_radio2()">男性&nbsp;
		<input type="radio" name="chk_info2" value="1" onclick="chk_radio2()">女性&nbsp;
		<input type="radio" name="chk_info2" value="2" onclick="chk_radio2()">中性(普通ではない)&nbsp;<br />
        <script language="javascript">

		var flag4 = 0;
		var flag5 = 0;
		var flag6 = 0;

			function chk_radio2(){
				if(document.all.chk_info2[0].checked == true){
					flag4 = 1;
					flag5 = 0;
					flag6 = 0;
					check_ok();
				}else if(document.all.chk_info2[1].checked == true){
					flag4 = 0;
					flag5 = 1;
					flag6 = 0;
					check_ok();
				}else if(document.all.chk_info2[2].checked == true){
					flag4 = 0;
					flag5 = 0;
					flag6 = 1;
					check_ok();
				}
			}



			function check_ok(){
				if((flag1||flag2)&&(flag4||flag5||flag6)){
					var input_ok = document.getElementById("ok");
					input_ok.innerHTML = "<br /><form name='myForm3' method='post'>"+
					"&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;ニックネーム：<input type='text' name='name3' size=8 maxlength=6  onKeyDown='javascript:if (event.keyCode == 13){ window.event.returnValue = false;}'><br />"+
					"&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;挨拶文：<textarea name='name4' rows='4' cols='25'></textarea><br />"+
					"&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;<p style='font-size:12px; display:inline;'>*ニックネームと挨拶文は必須項目ではありません</p><br /><br />"+
					"&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;"+
					"<input type='button' value='追加する' name='submitok' onclick='javascript:check_submit3();'> </form>";
				}else{
					var input_ok = document.getElementById("ok");
						input_ok.innerHTML = "";
				}
			}
        </script>
        <div id="ok"></div><br />
        <p style="display:inline;">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</p><p style="font-size:11px; text-decoration:underline; color:#FF0000; display:inline;">修正は出来ない</p><p style="font-size:11px; color:#FF0000; display:inline;">ので、慎重にお願いします。</p>
